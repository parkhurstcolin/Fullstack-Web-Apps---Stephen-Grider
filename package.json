{
	"name": "server",
	"version": "1.0.0",
	"main": "index.js",
	"engines": {
		"node": ">=20.14.0",
		"npm": ">=10.7.0"
	},
	"scripts": {
		"start": "nodemon index.js",
		"server": "nodemon ssl-index.js",
		"client": "npm run start --prefix client",
		"clientprod": "npm run start HTTPS=true SSL_CRT_FILE=process.env.SSL_CRT_FILE SSL_KEY_FILE=process.env.SSL_KEY_FILE --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"prod": "concurrently \"npm run server\" \"npm run clientprod\""
	},
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"body-parser": "^1.20.3",
		"concurrently": "^8.2.2",
		"cookie-session": "^2.1.0",
		"express": "^4.19.2",
		"http-proxy-middleware": "^2.0.6",
		"mongodb": "^6.8.0",
		"mongoose": "^8.5.3",
		"nodemon": "^3.1.4",
		"passport": "^0.5.0",
		"passport-google-oauth20": "^2.0.0",
		"stripe": "^16.12.0"
	}
}
